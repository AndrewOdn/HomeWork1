print("Введите день:")
a = int(input())
print("Введите месяц:")
b = str(input())
if b.lower() == "март":
    if a >= 21:
        print("Результат:\nВаш знак зодиака: Овен")
    else:
        print("Результат:\nВаш знак зодиака: Рыбы")
if b.lower() == "апрель":
    if a >= 21:
        print("Результат:\nВаш знак зодиака: Телец")
    else:
        print("Результат:\nВаш знак зодиака: Овен")
if b.lower() == "май":
    if a >= 21:
        print("Результат:\nВаш знак зодиака: Близнецы")
    else:
        print("Результат:\nВаш знак зодиака: Телец")
